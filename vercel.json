{
  "version": 2,
  "builds": [
    {
      "src": "dist/chat-ai/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/chat-ai/$1"
    }
  ]
}
